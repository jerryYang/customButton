        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".activities.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.RapidSwipeActivity"/>
        <activity android:name=".activities.SlowSwipeActivity"/>
        <activity android:name=".activities.TopBounceActivity"/>
        <activity android:name=".activities.DemoActivity"/>
        <activity android:name=".activities.CubicFreehandDrawActivity"/>
        <activity android:name=".activities.CubicDrawActivity"/>
        <activity android:name=".activities.SiblingSwipeThroughActivity"/>
        <activity android:name=".activities.WaterWaveActivity"/>
        <activity android:name=".activities.SmoothWaterWaveActivity"/>
        <activity android:name=".activities.CustomButtonActivity"/>
    </application>

</manifest>
